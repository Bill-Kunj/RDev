new
   stdout(`rho:io:stdout`),
   deployId(`rho:rchain:deployId`),
   deployerId(`rho:rchain:deployerId`),
   lookup(`rho:registry:lookup`),
   inboxCh
in {
   lookup!(toInboxURI, *inboxCh) |
   for (toinbox <- inboxCh) {
      stdout!(["toinbox",*toinbox]) |
      toinbox!(["wanna","CallForHelp", {"from": from, "tips": tips, "bounty": revletts, "body": body}], *deployId)
   }
}
