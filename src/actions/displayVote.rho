match ["inbox", ""] {
[lockerTag, issue] => {
new
   return(`rho:rchain:deployId`),
   deployerId(`rho:rchain:deployerId`),
   ch
in {
   for(@{"peek": *peek, ..._} <<- @[*deployerId, lockerTag]) {
      peek!("vote", issue, *ch) |
      for (@[{"voterCap": voterCapability}] <- ch) {
         @voterCapability!("choice", Set(), *return, *return)
      }
   }
}

}} // end of match
